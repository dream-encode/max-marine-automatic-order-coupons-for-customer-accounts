parameters:
    level: 8
    treatPhpDocTypesAsCertain: false
    checkMissingIterableValueType: false
    checkUninitializedProperties: false
    tmpDir: tests
    paths:
        - max-marine-automatic-order-coupons-for-customer-accounts.php
        - includes
        - admin
        - public
    scanFiles:
        - includes/max-marine-automatic-order-coupons-for-customer-accounts-constants.php
    scanDirectories:
        - includes
    excludePaths:
        - node_modules/*
    bootstrapFiles:
        - includes/max-marine-automatic-order-coupons-for-customer-accounts-constants.php
        - %rootDir%/../../php-stubs/wordpress-stubs/wordpress-stubs.php
        - %rootDir%/../../php-stubs/woocommerce-stubs/woocommerce-stubs.php
        - %rootDir%/../../php-stubs/woocommerce-stubs/woocommerce-packages-stubs.php
    ignoreErrors:
        -
            message: '#Variable \$[a-zA-Z0-9\\_]+ might not be defined#'
            paths:
                - admin/partials/*